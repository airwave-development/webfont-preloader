{
    "esversion": 5,
    "node": true,
    "bitwise": true,
    "camelcase": true,
    "eqeqeq": true,
    "eqnull": true,
    "immed": true,
    "indent": 2,
    "newcap": true,
    "nonew": true,
    "noarg": true,
    "quotmark": "single",
    "regexp": true,
